<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User</title>
</head>
<style>
    table, th, td {
      border:1px solid black;
    }
    .btn-edit , .btn-delete {
        background-color:mediumaquamarine ;
        border: none;
        border-radius:3px ;
        cursor: pointer;
        width: 30px;
        text-decoration: none;
    }
    .btn-delete {
        background-color: rgb(244, 55, 55);
    }
    </style>
<body>
    <table style="width:100%">
        <tr>
          <th>Email</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Actions</th>
        </tr>

        <% for(var i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].email %></td>
              <td><%= dataTable[i].firstName %></td>
              <td><%= dataTable[i].lastName %></td>
              <td>
                <a href="/editcrud?id=<%= dataTable[i].id %>" class="btn-edit" type="button">Edit</a>
                <a href="/deletecrud?id=<%= dataTable[i].id %>" class="btn-delete" type="button">Delete</a>
              </td>
            </tr>
         <% } %>
      </table>
</body>
</html>